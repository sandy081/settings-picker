{
	"name": "settings-picker",
	"displayName": "settings-picker",
	"description": "Settings Picker",
	"version": "1.1.0",
	"publisher": "sandy081",
	"engines": {
		"vscode": "^1.18.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:setting.toggle",
		"onCommand:setting.update",
		"onCommand:setting.reset"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "setting.toggle",
				"title": "Toggle Setting"
			},
			{
				"command": "setting.update",
				"title": "Update Setting"
			},
			{
				"command": "setting.reset",
				"title": "Reset Setting"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "setting.toggle"
				},
				{
					"command": "setting.update"
				},
				{
					"command": "setting.reset"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.5.2",
		"vscode": "^1.1.5",
		"mocha": "^3.5.0",
		"@types/node": "^7.0.43",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"vscode-nls": "^2.0.2"
	}
}